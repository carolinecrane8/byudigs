<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - byudigs</title>
    @*<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>*@
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />


        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

        <link href="/lib/font-awesome/css/all.min.css" rel="stylesheet" />

        <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />*@

    <meta name="viewport" content="width =device-width, inital-scale=1" />

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <link href="/lib/font-awesome/css/all.min.css" rel="stylesheet" />

</head>

<body id="page-top">
    <style>
        body {background-color: white/*#39a9fd*/;}
    </style>
    <!-- Navigation-->
    <nav class="navbar navbar-light bg-light" @*id="mainNav"*@>
        @*<div class="container">*@
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">BYU DIGS</a>
         
            <div class="collapse navbar-collapse" id="navbarResponsive">
 
                <ul class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Explore
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item nav-link text-dark" asp-area="" asp-controller="Home" asp-action="BurialList">Burials</a>
                        <a class="dropdown-item nav-link text-dark" asp-area="" asp-controller="Home" asp-action="AddBurialSimple">Jamie's</a>
                        <a class="dropdown-item nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Tableau">Interactive Dashboard</a>
                        <a class="dropdown-item nav-link text-dark" asp-area="" asp-controller="Home" asp-action="About">About Us</a>
                        <a class="dropdown-item nav-link text-dark" target="_blank" href="https://religion.byu.edu/event/egyptian-excavation-exhibit">In Action</a>
                    </div>
                </ul>
                @*<a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage"> @User.Identity.Name</a>*@
  
            </div>
        @*</div>*@
    </nav>
    <!-- Masthead-->
    @RenderBody()
    <!-- Footer-->
    <footer style="white-space"><div>Copyright © BYU DIGS 2021</div></footer>
    <!-- Bootstrap core JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Third party plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <!-- Core theme JS-->
    <script src="js/site.js"></script>
    @await RenderSectionAsync("Scripts", required: false) 
</body>
</html>
